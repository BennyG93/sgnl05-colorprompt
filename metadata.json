{
  "name": "sgnl05-colorprompt",
  "version": "1.1.2",
  "author": "sgnl05",
  "summary": "Puppet module for adding colors to your bash prompt",
  "license": "Apache-2.0",
  "source": "git://github.com/sgnl05/sgnl05-colorprompt.git",
  "project_page": "https://github.com/sgnl05/sgnl05-colorprompt",
  "issues_url": "https://github.com/sgnl05/sgnl05-colorprompt/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6", "7" ]
    },
   {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "6", "7" ]
    },
   {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12.04", "14.04" ]
    }
 ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=2.7.20 <4.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.3.0 <5.0.0""
    }
  ]
}
